<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd">

    <changeSet id="createTable-procedimento" runOnChange="true" author="iago">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="procedimentos"/>
            </not>
        </preConditions>
        <createTable tableName="procedimentos">
            <column name="id" type="serial">
                <constraints primaryKey="true"/>
            </column>
            <column name="procedimento" type="integer"/>
        </createTable>
    </changeSet>

    <changeSet id="insert1" runOnChange="true" author="iago">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="procedimentos"/>
        </preConditions>
        <insert tableName="procedimentos">
            <column name="procedimento" value="1234"/>
        </insert>
    </changeSet>

    <changeSet id="insert22" runOnChange="true" author="iago">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="procedimentos"/>
        </preConditions>
        <insert tableName="procedimentos">
            <column name="procedimento" value="4567"/>
        </insert>
    </changeSet>

    <changeSet id="insert33" runOnChange="true" author="iago">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="procedimentos"/>
        </preConditions>
        <insert tableName="procedimentos">
            <column name="procedimento" value="6789"/>
        </insert>
    </changeSet>
</databaseChangeLog>